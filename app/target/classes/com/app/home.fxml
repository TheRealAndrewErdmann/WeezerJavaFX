<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import java.net.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.HomeController">
   <children>
      <Label text="PMSoftware" />
      <Button fx:id="primaryButton" onAction="#switchToLogin" text="Log in" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Or" />
      <Button mnemonicParsing="false" text="Sign up" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <stylesheets>
        <URL value="@styles.css" />
   </stylesheets>
</VBox>
